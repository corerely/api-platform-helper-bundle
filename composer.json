{
    "name": "corerely/api-platform-helper-bundle",
    "description": "Api Platform helper filters etc..",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Corerely\\ApiPlatformHelperBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Corerely\\ApiPlatformHelperBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Andrii Oprysko",
            "email": "anjey.link@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "api-platform/doctrine-orm": "^4.0",
        "api-platform/symfony": "^4.0",
        "symfony/http-kernel": "^6.4 || ^7.0",
        "doctrine/orm": "*"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "*",
        "slevomat/coding-standard": "^8.0",
        "phpunit/phpunit": "^11.0",
        "symfony/framework-bundle": "^6.4 || ^7.0",
        "symfony/phpunit-bridge": "^6.4 || ^7.0",
        "symfony/property-access": "^6.4 || ^7.0",
        "symfony/yaml": "^6.4 || ^7.0",
        "symfony/uid": "^6.4 || ^7.0",
        "zenstruck/foundry": "^1.3 || ^2.0",
        "matthiasnoback/symfony-dependency-injection-test": "^5.0",
        "symfony/http-client": "^6.4 || ^7.0"
    },
    "config": {
        "allow-plugins": true
    }
}
